{
  "name": "n_abas",
  "manifest_version": 3,
  "version": "1.0",
  "author": "everton-tenorio",
  "description": "Uma extensão para capturar urls de abas do Chrome e salvá-las em arquivo .txt",
  "permissions": ["tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
